{
  "name": "monthpicker-vjs",
  "version": "1.0.1",
  "description": "Month picker component for Vanilla JS & TypeScript",
  "type": "module",
  "main": "./dist/monthpicker.umd.cjs",
  "module": "./dist/monthpicker.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/monthpicker.js",
      "require": "./dist/monthpicker.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc",
    "build": "tsup",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\"",
    "lint:fix": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaseottofy/monthpicker-vjs.git"
  },
  "keywords": [
    "datepicker",
    "monthpicker",
    "vanilla",
    "typescript",
    "javascript",
    "js",
    "ts"
  ],
  "author": "Chase Ottofy",
  "license": "MIT",
  "homepage": "https://github.com/chaseottofy/monthpicker-vjs#readme",
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.46.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
